.login_head {
    width: 100%;
    height: 260px;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}

.login_head image {
    width: 80px;
    height: 80px;
    border-radius: 50%;
    margin: 60px auto;
}

.login_head button {
    margin: 10px auto;
    width: 90%;
}

.login_head view {
    margin: 10px auto;
}

.login_tips {
    width: 100%;
    height: 60px;
    text-align: center;
    color: #AAA;
}

.login_btn {
}

.login_btn button {
    width: 80%;
    margin: 0px auto;
    color: #FFF;
    background: #0000FF;
}